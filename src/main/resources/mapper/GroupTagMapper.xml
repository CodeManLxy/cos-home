<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cos.api.mapper.GroupTagMapper">

    <select id="selectAllTagById" resultType="com.cos.api.entity.CosTag">
        select * from t_cos_tag
        where id in (select a.tag_id from t_group_tag a where group_id = #{group_id})
    </select>

</mapper>